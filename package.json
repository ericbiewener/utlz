{
  "name": "utlz",
  "version": "2.2.0",
  "description": "Some very self-centered utilities.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/ericbiewener/utlz",
  "author": "Eric Biewener",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublish": "tsc",
    "prettier": "prettier --write \"**/*.{ts,tsx,js}\"",
    "eslint": "eslint --fix --ext .ts,.tsx ./src",
    "jest": "jest",
    "jest:watch": "yarn run jest --watch",
    "jw": "yarn run jest:watch"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-typescript": "7.7.2",
    "@types/execa": "2.0.0",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.7",
    "babel-jest": "^24.9.0",
    "eslint": "6.6.0",
    "eslint-config-ericbiewener-typescript": "5.3.2",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "prettier": "1.19.1",
    "typescript": "3.7.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "execa": "4.0.0"
  }
